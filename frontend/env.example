# Frontend Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001

# VideoSDK Configuration (for future integration)
NEXT_PUBLIC_VIDEOSDK_API_KEY=your-videosdk-api-key

# Stripe Configuration (for future payment integration)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# App Configuration
NEXT_PUBLIC_APP_NAME=ACT Coaching For Life
NEXT_PUBLIC_APP_URL=http://localhost:4000

# Analytics Configuration (optional)
NEXT_PUBLIC_GA_TRACKING_ID=your-google-analytics-id
NEXT_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token

# Feature Flags
NEXT_PUBLIC_ENABLE_PAYMENTS=false
NEXT_PUBLIC_ENABLE_VIDEO_SESSIONS=false
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true

# Development Configuration
NEXT_PUBLIC_DEBUG_MODE=true
NEXT_PUBLIC_LOG_LEVEL=info 